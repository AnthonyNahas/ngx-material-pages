{
  "name": "demo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Demo app for @angular-material-extensions/pages",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4009",
    "serve:prod": "ng serve --prod",
    "build": "ng build",
    "build:lib": "ng build @angular-material-extensions/pages",
    "build:prod": "npm run link && ng build --prod --base-href /pages/ ",
    "build:prod:local": "npm run link && ng build --prod",
    "build:client-and-server-bundles": "ng build --prod && ng run demoapp:server:production",
    "build:client-and-server-bundles:local": "npm run build:prod:local && ng run demoapp:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:prerender:local": "npm run build:client-and-server-bundles:local && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "deploy:gh": "ngh --no-silent --dir=dist/browser",
    "deploy:firebase": "npm run build:prod && npm run build:doc && firebase deploy",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "link:pages": "cd dist/angular-material-extensions/pages && npm link",
    "link:lib": "npm link @angular-material-extensions/pages",
    "link": "npm run link:pages && npm run link:lib",
    "lint": "ng lint",
    "test": "ng test",
    "semantic-release": "semantic-release",
    "semantic-release-no-ci": "semantic-release --no-ci",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/platform-server": "^8.1.2",
    "@angular/router": "^8.1.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.1",
    "font-awesome": "^4.7.0",
    "include-media": "^1.4.9",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "^12.2.0",
    "ngx-disqus": "^2.4.2",
    "ngx-highlightjs": "^2.1.4",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.2",
    "@angular-devkit/build-ng-packagr": "^0.801.2",
    "@angular/cli": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@angular/service-worker": "^8.1.2",
    "@angularclass/hmr": "~2.1.3",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/exec": "^3.3.5",
    "@semantic-release/git": "^7.0.16",
    "@types/jasmine": "^2.8.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.14.13",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.0.1",
    "commitplease": "^3.2.0",
    "express": "^4.16.4",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.3.2",
    "reflect-metadata": "~0.1.10",
    "schematics-utilities": "^1.1.2",
    "semantic-release": "^15.9.17",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "~1.7.2",
    "xmlhttprequest": "^1.8.0"
  },
  "engines": {
    "node": ">=10.x"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/angular-material-extensions/pages"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "preset": "angular"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@types/jasmine",
      "@types/jest",
      "@types/node",
      "conventional-github-releaser",
      "rollup-plugin-uglify",
      "gulp-inline-ng2-template"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
